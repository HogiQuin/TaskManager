<div class="container">
  <h1>My Courses: </h1>
  <table class="table">
    <thead>
      <tr>
        <th>Course Name</th>
        <th>Semester</th>
      </tr>
    </thead>
    <tbody>
      <% @courses.each do |course| %>
      <% if course.semester.status == 1 %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.semester.name %></td>
      </tr>
      <% end %>
      <% end %>
    </tbody>
  </table>
